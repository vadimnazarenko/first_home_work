	<query><![CDATA[ 		
		CREATE TABLE IF NOT EXISTS `#__donate` (
		  `id` int(11) NOT NULL auto_increment,
		  `active` int(11) NOT NULL default '1',
		  `options` text,
		  PRIMARY KEY  (`id`)
		);
	]]></query>
	
	<query><![CDATA[ 		
