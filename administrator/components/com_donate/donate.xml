	]]></query>
		
	
	<query><![CDATA[ 
		INSERT INTO `#__donate` (`active`,`options`) VALUES (1, 'save_user_id=0,validate_user_account=1,force_https=0,select_joomfish_lang=,use_joomfish_lang=0,default_account_level=18,language_file=en-GB.php,block_user_account=1,show_user_account=0,create_user_account=0,admin_email=,sitename=My Site Name,processor=paypal_standard,gateway_mode=test,show_billing_address=1,show_email=1,show_phone=1,show_comments=0,show_organization=0,show_title=0,show_salutation=0,send_thankyou_email=0,send_admin_email=0,send_htmlemail=1,thank_you_body=Thank you for your donation!,thank_you_subject=Your Donation,thank_you_page=,thank_you_page_message=Thank you for your donation.,amounts=100.00|200.00|300.00,currency_descr=US,card_types=Visa|Mastercard,required_fields=firstname|lastname|card_type|credit_card|expiration_month|expiration_year|cvv|address1|city|_cd_state|postalcode|phone|email,auth_net_login=,auth_net_xaction=,wpp_user=,wpp_password=,wpp_signature=,payflow_user=,payflow_vendor=,payflow_partner=,payflow_password=,paypal_account=,show_country=1,show_credit=0,show_intro_text=1,intro_text=<h2>Donate Today</h2><p>Use the form below to make a donation.</p>,store_full_cc=0,');
	]]></query>
	<query><![CDATA[ 
		CREATE TABLE IF NOT EXISTS `#__donations` (
	  `id` int(11) NOT NULL auto_increment,
	  `firstname` varchar(255) NOT NULL,
	  `lastname` varchar(255) NOT NULL,
	  `title` varchar(255) NOT NULL,
	  `organization` varchar(255) NOT NULL,
	  `credit_card` varchar(16) NOT NULL,
	  `expiration` varchar(4) NOT NULL,
	  `expiration_month` varchar(4) NOT NULL,
	  `expiration_year` varchar(4) NOT NULL,	  
	  `cvv` varchar(3) NOT NULL,
	  `address1` varchar(255) NOT NULL,
	  `address2` varchar(255) NOT NULL,
	  `city` varchar(255) NOT NULL,
	  `state` varchar(255) NOT NULL,
	  `country` varchar(255) NOT NULL,
	  `postalcode` varchar(10) NOT NULL,
	  `email` varchar(255) NOT NULL,
	  `phone` varchar(255) NOT NULL,
	  `extension` varchar(255) NOT NULL,
	  `comments` text NOT NULL,
	  `amount` double NOT NULL,
	  `xaction_result` varchar(255) NOT NULL,
	  `xaction_id` varchar(255) default NULL,
	  `timestamp` datetime default NULL,
	  `card_type` varchar(255) default NULL,
	  `country_currency` varchar(5) default NULL,
	  `length` varchar(20) default NULL,
	  `unit` varchar(20) default NULL,
	  `duration` varchar(20) default NULL,
	  `configuration_id` int(11) default NULL,
	  `program` varchar(255) default NULL,
	  `invoice` varchar(255) default NULL,
	  `other` text NOT NULL,
	  `user_id` int(11) default NULL,
	  PRIMARY KEY  (`id`)
	);
	]]></query>
	
